{
  "name": "othello",
  "version": "1.0.0",
  "description": "pwd-othello",
  "main": "index.js",
  "dependencies": {
    "@yarnpkg/pnpify": "^2.3.3",
    "less": "^3.12.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "gh-pages": "^3.1.0",
    "terser": "^5.3.8"
  },
  "scripts": {
    "clear": "rm -rf dist && mkdir -p dist",
    "cp-index": "cp src/index.html dist",
    "babel": "babel src/othello-es6.js -o dist/othello.js",
    "less": "lessc src/othello.less dist/othello.css",
    "build": "yarn clear && yarn cp-index && yarn babel && yarn less",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "game"
  ],
  "author": "Ferry Irawan",
  "license": "ISC"
}
